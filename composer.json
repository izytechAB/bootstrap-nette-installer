{
    "name": "izytechab/bootstrap-nette-installer",
    "description": "Composer post-install helper for Twitter Bootstrap 3.x in Nette",
    "keywords": ["installer", "twitter-bootstrap", "nette"],
    "license": "GPL 3.0",
    "require": {
        "twitter/bootstrap": "3.*"
    },
    "scripts": {
        "post-install-cmd": "izytechab\\bootstrapNetteInstaller\\Composer\\ScriptHandler::postInstall",
        "post-update-cmd": "izytechab\\bootstrapNetteInstaller\\Composer\\ScriptHandler::postUpdate"
    },
    "autoload": {
        "psr-0": {"": "/src"}
    },
    "extra": {
        "nette-web-dir": "www"
    }
}
